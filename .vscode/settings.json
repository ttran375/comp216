{
    "git.autofetch": true,
    "gitdoc.enabled": true,
    "gitdoc.autoCommitDelay": 60000,
    "gitdoc.autoPushDelay": 0,
    "gitdoc.commitMessageFormat": "ccc, LLL d, y, h:mm a '+00:00'",
    "flake8.args": [
        "--ignore=E302,E402,E501,E722,F401,F403,F405,F541,F811,F841"
    ],
    "editor.tabSize": 2,
    "files.insertFinalNewline": true,
    "mypy-type-checker.args": [
        "import-not-found"
    ]
}
